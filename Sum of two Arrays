#include <iostream>  
using namespace std;  
  
int main() {    
    int myArray[5];  
  
    myArray[0] = 1;  
    myArray[1] = 2;  
    myArray[2] = 3;  
    myArray[3] = 4;  
    myArray[4] = 5;  
  
    
    for (int i = 0; i < 5; i++) {  
        cout << myArray[i] << " ";  
    }  
    cout << endl;  
  
    myArray[2] = 6;  
  
    for (int i = 0; i < 5; i++) {  
        cout << myArray[i] << " ";  
    }  
    cout << endl;  
    int anotherArray[5] = {10, 9, 8, 7, 6};  
   
    for (int i = 0; i < 5; i++) {  
        cout << anotherArray[i] << " ";  
    }  
    cout << endl;  
  
    return 0;  
}  
